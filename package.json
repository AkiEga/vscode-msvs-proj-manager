{
	"name": "vscode-msvs-proj-manager",
	"displayName": "vscode-msvs-proj-manager",
	"description": "",
	"publisher": "akiega",
	"version": "0.1.1",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/AkiEga/vscode-msvs-proj-manager"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:slnExplorer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-msvs-proj-manager.read-sln-file",
				"title": "read sln file"
			},
			{
				"command": "vscode-msvs-proj-manager.open-terminal-nearby-msvs-proj",
				"title": "open terminal nearby msvs proj"
			},
			{
				"command": "vscode-msvs-proj-manager.build-msvs-proj",
				"title": "build msvs proj"
			},
			{
				"command": "vscode-msvs-proj-manager.clean-msvs-proj",
				"title": "clean msvs proj"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "slnExplorer",
					"name": "sln explorer"
				}
			]
		},
		"menus": {
			"view/item/context": [
				{
					"command": "vscode-msvs-proj-manager.open-terminal-nearby-msvs-proj",
					"when": "view == slnExplorer"
				},
				{
					"command": "vscode-msvs-proj-manager.build-msvs-proj",
					"when": "view == slnExplorer"
				},
				{
					"command": "vscode-msvs-proj-manager.clean-msvs-proj",
					"when": "view == slnExplorer"
				}
			]
		}
	},
	"scripts": {
		"vscode:publish": "vsce package;vsce publish patch",
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.0",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2"
	},
	"dependencies": {
		"iconv-lite": "^0.5.0"
	}
}
