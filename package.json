{
	"name": "vscode-msvs-proj-manager",
	"displayName": "vscode-msvs-proj-manager",
	"description": "",
	"publisher": "akiega",
	"version": "0.1.35",
	"license": "MIT",
	"repository": {
		"url": "https://github.com/AkiEga/vscode-msvs-proj-manager"
	},
	"engines": {
		"vscode": "^1.39.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onView:slnExplorer"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-msvs-proj-manager.read-sln-file",
				"title": "ðŸ“‘Read a msvs solution file(.sln)"
			},
			{
				"command": "vscode-msvs-proj-manager.open-terminal-nearby-msvs-proj",
				"title": "Open terminal nearby the msvs project"
			},
			{
				"command": "vscode-msvs-proj-manager.build-msvs-proj-with-active-config",
				"title": "ðŸ”¨Build a msvs project (auto config)"
			},
			{
				"command": "vscode-msvs-proj-manager.build-msvs-proj",
				"title": "ðŸ”¨Build a msvs project (manual config)"
			},
			{
				"command": "vscode-msvs-proj-manager.clean-msvs-proj",
				"title": "ðŸ§¹Clean a msvs project"
			}
		],
		"views": {
			"explorer": [
				{
					"id": "slnExplorer",
					"name": "msvs solution explorer"
				}
			]
		},
		"menus": {
			"view/item/context": [
				{
					"command": "vscode-msvs-proj-manager.build-msvs-proj-with-active-config",
					"when": "view == slnExplorer"
				},
				{
					"command": "vscode-msvs-proj-manager.open-terminal-nearby-msvs-proj",
					"when": "view == slnExplorer"
				},
				{
					"command": "vscode-msvs-proj-manager.build-msvs-proj",
					"when": "view == slnExplorer"
				},
				{
					"command": "vscode-msvs-proj-manager.clean-msvs-proj",
					"when": "view == slnExplorer"
				}
			]
		}
	},
	"scripts": {
		"vscode:prepublish": "webpack --mode production",
		"webpack": "webpack --mode development",
		"webpack-dev": "webpack --mode development --watch",
		"publish": "powershell -Command {vsce publish patch -p $(cat ./PublishAccessToken.txt)}",
		"test-compile": "tsc -p ./"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.6",
		"@types/node": "^10.17.0",
		"@types/vscode": "^1.39.0",
		"glob": "^7.1.4",
		"mocha": "^6.1.4",
		"ts-loader": "^6.2.1",
		"tslint": "^5.12.1",
		"typescript": "^3.3.1",
		"vscode-test": "^1.0.2",
		"webpack": "^4.41.2",
		"webpack-cli": "^3.3.10"
	},
	"dependencies": {
		"dotenv": "^8.2.0",
		"iconv-lite": "^0.5.0"
	}
}
